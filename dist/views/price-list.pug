extends layout

block vars
  - var pageTitle= 'CarWellness | Cenník'

block css
  include main-styles.pug

block content
  style.
    .social-icon {
      font-size: 21px;
    }
    .top-150 {
      top: 150px;
    }
     
  #wrapper
    include navbar.pug

    .container-fluid.serviceshead
      img.headpicture(data-src='./assets/images/price-listhead.jpg' alt='')
      .container.headContainer
        h3 Cenník služieb
        
    .container.price-listContent
      h4 Pozrite si cenník služieb a programov.
      p Ak chcete viac informácií ohľadom naších služieb neváhajte nás kontaktovať.
    
    .container.price-list-price
      .card-deck
        .card
          .card-body
            .card-body-header
              h4 COMFORT
              h5 Čas trvania: #[strong 1h 30m]
            .card-body-body
              p.light-grey Služba obsahuje kombináciu programov #[strong EXTERIÉR a INTERIÉR]
              p.price 28,00 €
              a(href='./online-objednavka') Objednať online
        .card
          .card-body
            .card-body-header
              h4 EXCLUSIVE
              h5 Čas trvania: #[strong 4 hodiny]
            .card-body-body
              p.light-grey Služba obsahuje program #[strong COMFORT]
              p Vyčistenie a vyleštenie okien
              p.light-grey Ošetrenie všetkých plastov interiéru
              p Tepovanie / čistenie interiéru (záleží od materiálov použitých v interiéri) vrátane batožinového priestoru, všetkých kobercov, sedadiel a operadiel
              p.light-grey Vyčistenie a leštenie čelného skla
              p.price 78,00 €
              a(href='./online-objednavka') Objednať online
        .card
          .card-body
            .card-body-header
              h4 EXTERIÉR
              h5 Čas trvania: #[strong 45 minút]
            .card-body-body
              p.light-grey Nanesenie chémie na predumytie vozidla
              p Odstránenie nečistôt silným tlakom
              p.light-grey Čistenie diskov kolies
              p Ručné umývanie šampónom
              p.light-grey Samosušiaci vosk
              p Sušenie karosérie a všetkých vnútroných hrán vrátane použitia kompresora
              p.light-grey Ošetrenie podbehov a pneu
              p.price 15,00 €
              a(href='./online-objednavka') Objednať online
        .card
          .card-body
            .card-body-header
              h4 INTERIÉR
              h5 Čas trvania: #[strong 45 minút]
            .card-body-body
              p.light-grey Luxovanie interiéru a batožinového priestoru
              p Suché čistenie interiéru a batožinového priestoru
              p.light-grey Ošetrenie všetkých plastov interiéru
              p Prevoňanie interiéru
              p.light-grey Vyčistenie a leštenie čelného skla
              p.price 18,00 €
              a(href='./online-objednavka') Objednať online
    
    .container.price-list-price.top-240
      .card-deck
        .card
          .card-body
            .card-body-header
              h4 PRÉMIUM EXTERIÉR
              h5 Čas trvania: #[strong 12 hodín]
            .card-body-body
              p.light-grey Umývanie motora a ošetrenie motorových plastov, nanesenie chémie na predumytie vozidla
              p Detailné odstránenie nečistôt silným tlakovým umytím, čistenie diskov a kolies, ručné umývanie šampónom
              p.light-grey Detailné odstránenie asfaltu z karosérie a kolies, odmastnenie karosérie a starých voskov, živice a prilepených kvetov
              p Detailné clay-ovanie, sušenie karosérie a všetkých vnútroných hrán vrátane použitia kompresora
              p.light-grey Ošetrenie vonkajších plastových, chromovaných a humových dielov, podbehov a pneu
              p Ručné leštenie čistiacou a leštiacou pastou s voskom, nanesenie tvrdého karnaubského vosku(produkt ponúka ochranu trvajúcu až 6 mesiacov)
              p.light-grey Tekuté stierače
              p.price 108,00 €
              a(href='./online-objednavka') Objednať online
        .card
          .card-body
            .card-body-header
              h4 PREMIUM INTERIÉR
              h5 Čas trvania: #[strong 12 hodín]
            .card-body-body
              p.light-grey Detailné luxovanie interiéru vrátane batožinového priestoru
              p Detailné suché čistenie interiéru vrátane batožinového priestoru
              p.light-grey Detailné ošetrenie všetkých plastov interiéru, vyčistenie a vyleštenie okien
              p Použitie tornádora, tepovanie interiéru vrátane batožinového priestoru, všetkých kobercov, všetkých sedadiel a operadiel
              p.light-grey Tepovanie stropu, sušenie interiéru, detailné hĺbkové vyčistenie a ošetrenie kožených sedadiel, nanesenie vyživujúcej emulzie
              p.price 108,00 €
              a(href='./online-objednavka') Objednať online
        .card
          .card-body
            .card-body-header
              h4 AVANGARDE
              h5 Čas trvania: #[strong 2h 15 minút]
            .card-body-body
              p.light-grey Služba obsahuje program #[strong COMFORT]
              p Vyčistenie a vyleštenie okien
              p.light-grey Vyčistenie a leštenie čelného skla
              p.price 58,00 €
              a(href='./online-objednavka') Objednať online
        .card
          .card-body
            .card-body-header
              h4 TOP GLANZ
              h5 Čas trvania: #[strong 3 hodiny]
            .card-body-body
              p.light-grey Služba obsahuje program #[strong COMFORT]
              p Vyčistenie a vyleštenie okien
              p.light-grey Ošetrenie vonkajších plastových, chromovaných a gumových dielov
              p Ručné leštenie čistiacou a leštiacou pastou s voskom
              p.light-grey (Produkt ponúka ochranu trvajúcu 2 mesiace pred znečistením vonkajšími vplyvmi)
              p.price 68,00 €
              a(href='./online-objednavka') Objednať online
    
    .container.price-listContent.added-services
      h4 Doplnkové služby
      .row
        .col-lg-6
          h5 EXTERIÉR
          .row
            .col-sm-9
              p -odstránenie asfaltu z karosérie a kolies 
              p -odstránenie živice a prilepených kvetov 
              p -detailné clay-ovanie 
              p -umytie motora a ošetrenie motorových plastov 
              p -tekuté stierače - NANO4life
              p -renovácia svetlometov 1ks 
              p -aplikácia leštiacej pasty s voskom 
            .col-sm-3
              p #[strong 12€ /24€ /48€]
              p #[strong 12€ /24€ /48€]
              p #[strong 24€ /36€]
              p #[strong 13€]
              p #[strong 15€ /28€]
              p #[strong 25€]
              p #[strong 45€]
        .col-lg-6
          h5 INTERIÉR
          .row
            .col-sm-11
              p -tepovanie stropu 
              p -tepovanie sedadla 
              p -tepovanie sedadiel a operadiel 
              p -hĺbkové vyčistenie a ošetrenie kožených sedadiel,
              p nanesenie vyživujúcej emulzie 
              p -použitie tornádora 
              p -dezinfekcia klimatizácie prostriedkom OdorBact Out 
              p -dezinfekcia klimatizácie a interiéru vozidla generátorom ozónu 
              p -aplikácie nanotechnológie SmartCoat do interiéru vozidla 
              p (prevencia pred vírusovými chorobami, riešenie pre alergikov<br>a masívna eliminácia výskytu mikroorganizmov)
            .col-sm-1
              p #[span 18€]
              p #[strong 12€]
              p #[strong 48€]
              p &nbsp;
              p #[strong 48€]
              p #[strong 15€]
              p #[strong 12€]
              p #[strong 20€]
              p #[strong 88€]
    
    section.gallery.top-260
      .container
        h3 Galéria
        p Zoznámte sa s našou prácou, ktorú sme odovzdali spokojným klientom.
        .row
          .col-lg-12
            a(href='./galeria')
              img(data-src='./assets/images/gallerybentley.jpg' alt='')
            h4 CARwellness Žilina & Nitra
    
    .position-relative.top-150
      include footer.pug
block script
  include main-scripts.pug
  script(src='./assets/js/carwellness-main.min.js')