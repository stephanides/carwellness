extends layout

block vars
  - var pageTitle= 'CARwellness - profesionálna autoumyváreň | Galéria'

block css
  include main-styles.pug
  link(rel='preload' href='https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.3.5/jquery.fancybox.min.css' as='style' onload='this.rel="stylesheet"' media='screen')
